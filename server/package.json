{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && npx tsc",
    "dev": "npm run build && concurrently \"npx tsc -w\" \"nodemon --exec ts-node src/index.ts\"",
    "start": "npm run build && node dist/src/index.js",
    "prisma:reset": "npx prisma migrate reset",
    "postprisma:generate": "shx cp -r node_modules/.prisma/client/index.d.ts ../client/src/types/prismaTypes.d.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "npx prisma migrate dev --name init",
    "prisma:deploy": "npx prisma migrate deploy",
    "prisma:seed": "ts-node prisma/seed.ts",
    "setupdb:dev": "npm run prisma:generate && npm run prisma:migrate && npm run prisma:seed",
    "setupdb:staging": "npm run prisma:generate && npm run prisma:deploy && npm run prisma:seed",
    "setupdb:prod": "npm run prisma:generate && npm run prisma:deploy && npm run prisma:seed"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@terraformer/wkt": "^2.2.1",
    "@aws-sdk/client-s3": "^3.864.0",
    "@aws-sdk/lib-storage": "^3.864.0",
    "@prisma/client": "^6.14.0",
    "prisma": "^6.14.0",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^11.1.0",
    "axios": "^1.11.0",
    "express": "^5.1.0",
    "dotenv": "^17.2.1",
    "helmet": "^8.1.0",
    "morgan": "^1.10.1",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "multer": "^2.0.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.3.0",
    "@types/terraformer__wkt": "^2.0.3",
    "@types/uuid": "^10.0.0",
    "concurrently": "^9.2.0",
    "rimraf": "^6.0.1",
    "shx": "^0.4.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}